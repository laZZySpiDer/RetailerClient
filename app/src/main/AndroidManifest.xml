<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aditya.retailerclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher_icon"
        android:label="Shree Hari Homes"
        android:roundIcon="@mipmap/launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Dashboard" />
        <activity android:name=".LoginPage" />
        <activity
            android:name=".CartActivity"
            android:label="@string/title_activity_cart"
            android:theme="@style/AppTheme" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ProductDetails" />
        <activity android:name=".ParentWiseProducts" />
        <activity android:name=".WalkThrough">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditPersonalInfo" />
        <activity android:name=".EditOtherInfo" />
        <activity android:name=".ChangePassword"></activity>
    </application>

</manifest>